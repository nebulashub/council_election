{
  "name": "nebulas/neb.php",
  "description": "Nebulas sdk implemented with PHP",
  "license": "MIT",
  "require": {
    "PHP": "^7.1",
    "stephenhill/base58": "~1.0",
    "grpc/grpc": "^v1.3.0",
    "google/protobuf": "^v3.3.0",
    "simplito/elliptic-php":"1.0.4",
    "litipk/php-bignumbers": "^0.8.6",
    "ext-curl": "*",
    "ext-gmp": "*",
    "ext-scrypt":"*"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Nebulas\\": "src/",
      "Corepb\\": "src/proto/dist/Corepb",
      "GPBMetadata\\":"src/proto/dist/GPBMetadata",
      "Rpcpb\\":"src/proto/dist/Rpcpb"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}
